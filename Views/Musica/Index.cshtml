@model List<Musica>

<a asp-action="Create">Criar</a>

<table class="text-white">
    <thead><tr><th scope="col">id</th><th scope="col">nome</th><th scope="col">letra</th><th scope="col">Editar</th><th scope="col">Excluir</th></tr></thead>
    <tbody>
        @foreach (var musica in Model)
        {
            <tr><th scope="row">@musica.IdMusica</th><td>@musica.Nome</td><td>@musica.Letra</td><td><a asp-action="Edit" asp-route-id="@musica.IdMusica">Editar</a></td><td><a asp-action="Delete" asp-route-id="@musica.IdMusica">Excluir</a></td></tr>
        }
    </tbody>
</table>